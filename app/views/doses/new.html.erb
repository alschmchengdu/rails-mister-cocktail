<div class="container">
  <div class="row">
    <div class="col col-xs-12 col-xs-12 col-sm-6 col-sm-offset-3">
      <h1>New Dose</h1>
      <%= simple_form_for [@cocktail, @dose] do |f| %>
        <%= f.input :ingredient, collection: Ingredient.all %>
        <%= f.input :description %>
        <%= f.button :submit %>
      <% end %>
    </div>
  </div>
</div>
